метод ГодПредставлениеЗаголовок(): Строка
    возврат Строки.Шаблон("$0 год", Год.ВСтроку())
;

метод НазадПриНажатии(Источник: Картинка, Событие: СобытиеПриНажатии)
    если Год == 1
        возврат
    ;
    Год -= 1
;

метод ВпередПриНажатии(Источник: Картинка, Событие: СобытиеПриНажатии)
    Год += 1
;