// JSON

@ВПодсистеме
структура ДанныеJson_V2_0
    обз знч data: Строка
    обз знч actual: Булево
;

@ВПодсистеме
метод ЗаполнитьПоJson(ДанныеJson: ДанныеJson_V2_0)
    Данные = ДанныеJson.data
    Актуальный = ДанныеJson.actual
;

@ВПодсистеме
метод ПолучитьJson_V2_0(): ДанныеJson_V2_0
